<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<% urls.forEach(url => { %>
	<url>
		<% if (locals.build) { %>
		<% if (!url.urlLocation.endsWith("/")) { %>
		<loc><%= url.urlLocation + ".html" %></loc>
		<% } else { %>
		<loc><%= url.urlLocation %></loc>
		<% } %>
		<% } else { %>
		<loc><%= url.urlLocation %></loc>
		<% } %>
		<lastmod><%= url.urlLastMod %></lastmod>
	</url>
	<% }) %>
</urlset>
